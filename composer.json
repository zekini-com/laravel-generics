{
    "name": "zekini/laravel-generics",
    "description": "A simple package to encapsulate all of our generic classes at Zekini to a package we can require in all our projects",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Keania Niate Eric",
            "email": "keania@zekini.com"
        },
        {
            "name": "Llewellyn du Randt",
            "email": "llewellyn@zekini.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "barryvdh/laravel-dompdf": "^1.0",
         "doctrine/dbal": "^3.3",
        "fakerphp/faker": "^1.19",
        "laravel/jetstream": "^2.7",
        "livewire/livewire": "^2.10",
        "mediconesystems/livewire-datatables": "^0.9",
        "spatie/laravel-activitylog": "^4.5",
        "spatie/laravel-permission": "^5.5",
        "maatwebsite/excel": "^3.1.40"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.1",
        "orchestra/testbench": "^7.4",
        "phpunit/phpunit": "^9.5",
        "protoqol/prequel": "^1.23",
        "symplify/easy-coding-standard": "^10.0",
        "vimeo/psalm": "^4.23"
    },
    "autoload": {
        "psr-4": {
            "Zekini\\Generics\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zekini\\Generics\\GenericsServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zekini\\Generics\\Tests\\": "tests"
        }
    }
}
