{
    "name": "zekini/laravel-generics",
    "description": "A simple package to encapsulate all of our generic classes at Zekini to a package we can require in all our projects",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Keania Niate Eric",
        "email": "support@zekini.com"
    }],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "arcanedev/log-viewer": "dev-master",
        "doctrine/dbal": "^3.2",
        "fakerphp/faker": "^1.16",
        "laravel/jetstream": "3.x-dev",
        "livewire/livewire": "^2.7",
        "mediconesystems/livewire-datatables": "dev-master",
        "owen-it/laravel-auditing": "^12.0",
        "spatie/laravel-backup": "dev-master",
        "spatie/laravel-permission": "^5.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.6.x-dev",
        "nunomaduro/collision": "dev-stable",
        "orchestra/testbench": "6.x-dev",
        "phpunit/phpunit": "9.5.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Zekini\\Generics\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zekini\\Generics\\GenericsServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zekini\\Generics\\Tests\\": "tests"
        }
    }
}
